<head>
  <meta rel="manifest" href="/manifest.webmanifest">
  <title>Kuilo</title>
  <%= csp_meta_tag %>
  <%= action_cable_meta_tag %>
  <%= csrf_meta_tags %>
  <%= render 'favicon' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
